# create jar file
birdplaydar.jar:
% content birdplaydar %content/
% locale birdplaydar en-US %locale/en-US/
% skin birdplaydar classic/1.0 %skin/
# overlays
% overlay chrome://songbird/content/xul/layoutWithBrowserOverlay.xul chrome://birdplaydar/content/birdplaydar-scripts.xul
% overlay chrome://songbird/content/xul/layoutWithBrowserOverlay.xul chrome://birdplaydar/content/birdplaydar-menu.xul
% overlay chrome://songbird/content/xul/layoutWithBrowserOverlay.xul chrome://birdplaydar/content/birdplaydar-commands.xul
# include all file in this jar
@include_all_to
